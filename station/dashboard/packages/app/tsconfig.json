{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "common": [
        "../common/src"
      ],
      "common/*": [
        "../common/src/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    },
    {
      "path": "../common"
    }
  ]
}